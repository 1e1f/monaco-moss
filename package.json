{
  "name": "monaco-moss",
  "version": "2.4.0",
  "description": "Moss plugin for the Monaco Editor, derived from monaco-yaml",
  "scripts": {
    "compile": "rimraf ./out && tsc -p ./tsconfig.json && tsc -p ./tsconfig.esm.json",
    "watch": "tsc -p ./src --watch",
    "compile:umd": "tsc -p ./src/tsconfig.json",
    "compile:esm": "tsc -p ./src/tsconfig.esm.json",
    "bundle": "rimraf ./lib && yarn bundle:umd && yarn bundle:esm && mcopy ./src/monaco.d.ts ./lib/monaco.d.ts",
    "bundle:umd": "node ./scripts/bundle-umd",
    "bundle:esm": "node ./scripts/bundle-esm",
    "build": "yarn compile && yarn bundle",
    "prepare": "yarn build",
    "lint": "prettier \"{src,test}/**/*.{json,scss,html,ts}\" --write",
    "test": "jest --verbose"
  },
  "main": "./lib/esm/monaco.contribution.js",
  "module": "./lib/esm/monaco.contribution.js",
  "typings": "./lib/monaco.d.ts",
  "directories": {
    "lib": "./lib"
  },
  "author": "Leif Shackelford <structuresound@gmail.com>",
  "maintainers": [
    "1e1f"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/1e1f/monaco-moss"
  },
  "bugs": {
    "url": "https://github.com/1e1f/monaco-moss/issues"
  },
  "devDependencies": {
    "@types/chai": "^4.1",
    "@types/mocha": "^5.2",
    "@types/node": "^10",
    "js-yaml": "^3.13",
    "js-moss": "^0.17",
    "jsonc-parser": "^2.2",
    "monaco-editor-core": "^0.20",
    "monaco-languages": "^1.10",
    "monaco-plugin-helpers": "^1.0.2",
    "requirejs": "^2.3",
    "rimraf": "^2.6",
    "typescript": "^3.8",
    "uglify-es": "^3.3",
    "vscode-nls": "^4",
    "vscode-json-languageservice": "^3.4",
    "vscode-languageserver-types": "3.15"
  }
}
