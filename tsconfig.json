{
  "compilerOptions": {
    "module": "umd",
    "moduleResolution": "node",
    "outDir": "./out/amd",
    "forceConsistentCasingInFileNames": true,
    "target": "es5",
    "lib": [
      "dom",
      "esnext"
    ],
    "downlevelIteration": true
  },
  "include": [
    "src"
  ],
  "exclude": [
    "out",
    ".vscode"
  ]
}